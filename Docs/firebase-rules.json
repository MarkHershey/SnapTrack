{
  "rules": {
    "users": {
      "$uid": {
        // Allow only authenticated content owners access to their data
        ".read": "auth.uid == $uid",
        ".write": "auth.uid == $uid"
      }
    },
    
    "userIDs": {
      "$id": {
        // Cannot delete once written
        ".read" : true,
        ".write" : "!data.exists()"
      }
    }
  }
}